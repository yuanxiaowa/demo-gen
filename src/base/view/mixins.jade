mixin script(items)
  if typeof items === 'string'
    - items = [items]
  each item in items
    if !/\.js|\}/.test(item)
      - item += '.js'
    script(src=item)
mixin style(items)
  if typeof items === 'string'
    - items = [items]
  each item in items
    if !/\.css|\}/.test(item)
      - item += '.css'
    link(rel="stylesheet", href=item)